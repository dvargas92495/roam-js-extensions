    "build:next": "next build",
    "postbuild:next": "next export",
    "prebuild:pr": "echo 'Building extensions: Process will take a couple of minutes...'",
    "build:pr": "webpack --config ./webpack.development.config.js",
    "presetup:local": "npm run build:pr",
    "setup:local": "node scripts/prelocal.js",
    "start": "next dev",
    "prelocal": "npm run setup:local",
    "local": "http-server ./",
    "test": "jest --passWithNoTests",
    "extension": "node scripts/extension.js",
    "digest": "node scripts/digest.js",
    "lint": "eslint . --ext .ts,.tsx",
    "prebuild": "npm install"
  },
